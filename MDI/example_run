uniqueIdentifier = 1111;
nComponents = 4;
nSamples    = 500;
hyperSamplingFrequency = 1;
samplingFrequency = 1;
initialise = true;
numbofparts = 20;
fileName1   = 'Data/datagen/gaussian1.csv';
fileName2   = 'Data/datagen/gaussian2.csv';
fileName3   = 'Data/datagen/multinom1.csv';
fileName4   = 'Data/datagen/multinom2.csv';
fileName1 = 'Data/synth_data/testData1.csv'
fileName2 = 'Data/synth_data/testData2.csv'
dataType1   = 'Gaussian';
dataType2   = 'Gaussian';
dataType3   = 'Multinomial';
dataType4   = 'Multinomial';
numbofparts = 1000;
MDI('test_results/', nComponents, nSamples, {fileName1, fileName2,fileName3, fileName4},...
    {dataType1, dataType2, dataType3, dataType4},...
    hyperSamplingFrequency,...
    samplingFrequency, uniqueIdentifier, initialise, numbofparts);

